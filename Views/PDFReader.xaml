<Window x:Class="CatApp.Views.PDFReader"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CatApp.Views"
        mc:Ignorable="d"
        Title="PDFReader" Height="450" Width="800">
    <Grid>
        <Frame x:Name="PDF" Margin="0,20,0,0" Grid.ColumnSpan="5"/>
        <Button x:Name="new" Content="new file" HorizontalAlignment="Left" Margin="20,0,0,0" VerticalAlignment="Top" Width="50" Click="New_Click" Height="20" Grid.ColumnSpan="3"/>
    </Grid>
</Window>
